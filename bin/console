#!/usr/bin/env php
<?php

require_once __DIR__ . '/../vendor/autoload.php';

set_time_limit(0);

use Slim\Commands\TestCommand;
use Slim\SlimPHP;



/** @var SlimPHP $app */
$app = require __DIR__ . '/../bootstrap.php';

$console = $app->getConsoleApplication();
$console->addCommands(
    [ 
        new TestCommand(),
    ]
);

$console->run();
